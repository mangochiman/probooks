  <div class="content">
    <div class="header">
      <h1 class="page-title"><%= @page_title %></h1>
      <ul class="breadcrumb">
        <li><a href="/dashboard">Home</a> </li>
        <li class="active"><%= @page_title %></li>
      </ul>
      <div class="row pull-right" style="margin-top: -31px;">
        <div class="col-md-12">
          <div class="input-group" id="adv-search">
            <form class="form-horizontal" role="form" method="GET" action="/search_results" id="general-search-form">
              <input type="text" name="general_search" id="general-search" class="form-control" placeholder="Search for Books" />
            </form>
            <div class="input-group-btn">
              <div class="btn-group" role="group">
                <div class="dropdown dropdown-lg">
                  <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><span class="caret"></span></button>
                  <div class="dropdown-menu dropdown-menu-right" role="menu">
                    <form class="form-horizontal" role="form" method="GET" action="/search_results" id="search-form">
                      <div class="form-group">
                        <label for="filter">Filter by</label>
                        <select class="form-control" name="search_type">
                          <option value="all" selected>Everything</option>
                          <option value="author">Book Author</option>
                          <option value="title">Book Title</option>
                        </select>
                      </div>
                      <div class="form-group">
                        <label for="contain">Contains the words</label>
                        <input class="form-control" type="text" name="search_words" />
                      </div>
                      <button type="submit" class="btn btn-primary"><span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
                    </form>
                  </div>
                </div>
                <button onclick="submitSearchForm()" type="button" class="btn btn-primary"><span class="glyphicon glyphicon-search" aria-hidden="true"></span></button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div><br />

    <div class="row" style="padding-right: 64px; top: -36px; position: relative;">

      <button class="btn btn-default btn-circle btn-xl pull-right main-icon" type="button" >
        <i class="glyphicon glyphicon-time"></i>
        <span class="icon-text" style="
              font-size: 12pt;
              position: absolute;
              padding-top: 47px;
              right: 70px;
              float: right;
              color: #5B5B5B;">
          Extras
        </span>
      </button>

      <button onclick="goToBookShelf();" class="btn btn-default btn-circle btn-xl pull-right main-icon" type="button" style="margin-right: 40px;">
        <i class="glyphicon glyphicon-search"></i>
        <span class="icon-text" style="
              font-size: 12pt;
              position: absolute;
              padding-top: 47px;
              right: 150px;
              float: right;
              color: #5B5B5B;">
          Book Shelf
        </span>
      </button>

      <button onclick="goToBookStore();" class="btn btn-default btn-circle btn-xl pull-right main-icon" type="button" style="margin-right: 40px;">
        <i class="glyphicon glyphicon-folder-open"></i>
        <span class="icon-text" style="
              font-size: 12pt;
              position: absolute;
              padding-top: 47px;
              right: 249px;
              float: right;
              color: #5B5B5B;">
          Book Store
        </span>
      </button>

      <br /><br /><br />
    </div>

    <div class="main-content">
      <div class="faq-content">

        <div class="row">
          <div class="col-sm-9 col-md-9">
            <% if flash[:notice] %>
              <div class="alert alert-success alert-dismissable">
                <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
                <%= flash[:notice] %>
              </div><br />
            <% end %>
            <% if flash[:error] %>
              <div class="alert alert-danger alert-dismissable">
                <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
                <%= flash[:error] %>
              </div><br />
            <% end %>
            <%= yield %>
          </div>
