  <script src="/AircraftAdminFree/lib/bootstrap/js/bootstrap.js"></script>
  <script type="text/javascript">
    $(function() {
      var uls = $('.sidebar-nav > ul > *').clone();
      uls.addClass('visible-xs');
      $('#main-menu').append(uls.clone());
    });

  </script>
  <script type="text/javascript">
    $("[rel=tooltip]").tooltip();

    function goToBookShelf(){
      window.location = "/dashboard";
    }
    function goToBookStore(){
      window.location = "/select_books_from_store";
    }

    function goToRecentBooks(){
      window.location = "/my_recent_books_menu";
    }

    function goToExtras(){
      window.location = "/extras";
    }

    $("#viewBook").draggable({
      handle: ".modal-header"
    });

    function submitSearchForm(){
      $('#general-search-form').submit();
    }

    function changeTextColor(){
      $(".main-icon").mouseover(function() {
        $(this).find('.icon-text').css( "color", "#000000" );
      });

      $(".main-icon").mouseout(function() {
        $(this).find('.icon-text').css( "color", "#5B5B5B" );
      });
    }

    changeTextColor();
  </script>


